<!--pages/ClockList/ClockList.wxml-->

  <view class="group_list_text" hidden='{{isBindExpert}}'>{{string}}</view>
  <scroll-view scroll-y="true" style="width:100%;">
    <navigator wx:for="{{clockList}}"  hover-class='navigator-hover' url="../ClockStart/ClockStart?clockName={{item.name}}&minutesLimit={{item.minutes}}">
     <view class="group_list" style="color:{{item.color}};">
        <view class="lable1">
          <text class="groupS1">{{item.name}}</text>
        </view>
        <view class="lable2">
          <view class="groupS3">{{item.minutes}}分钟</view>
        </view>
        <view class="lable3">
          <view class="groupS3">完成度：{{item.showCompletion}}%</view>
        </view>
        <view class="lable4">
          <button class="groupS4">开始</button>
        </view>
      </view>
    </navigator>
    </scroll-view>


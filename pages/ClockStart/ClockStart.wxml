<!--pages/ClockStart/ClockStart.wxml-->

<view class="title_goal">
  <text>目标:{{clockName}}</text>
</view>
<view class="time_area">
  <text class="time">{{minutes}}:{{seconds}}</text>

</view>

<view class="slider_area">
  <slider class="slider" min="{{minutesLimit}}" max="120" step="5" bindchanging="goal_of_time" value="{{minutes}}"></slider>
  <text>120</text>
   </view>

    <view class="btn-area">
      <button class="button" bindtap="button_distinguish" disabled='{{is_disabled}}'>{{buttonValue}}</button>
      </view>
<!--pages/GroupList/GroupList.wxml-->
<!--groupList.wxml-->
<view class="windows">
  <navigator wx:for="{{items}}" hover-class='navigator-hover' url='../GroupDetail/GroupDetail'>
    <view class='group_list'>
      <view class="group_list_text">{{item.groupName}}</view>
      <view class="group_list_text">{{item.groupDay}}</view>
      <view class="group_list_text">{{item.groupNumber}}</view>
    </view>
  </navigator>
</view>

<!--pages/GroupMange/GroupMange.wxml-->
<!--groupMange.wxml-->
<view class="weui-cells__title">小组名称</view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder="小组名称" />
    </view>
  </view>
</view>
<view class="weui-cells__title">成员管理</view>
<view class="group_member_list">
  <view class="group_member_icon" wx:for="{{items}}">{{item.nickname}}</view>
</view>
 <view class="weui-cell weui-cell_input">
    <view class="weui-cell__bd">
      <switch bindchange ='switchChange'class="is_private_text">设为私密</switch> 
    </view>
  </view>
<button class="weui-btn" type="default" disabled="true" plain="true" bindtap='deleteGroup'>解散小组</button>

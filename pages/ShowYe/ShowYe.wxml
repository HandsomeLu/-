<!--搜索框-->
  <view class="search">
    <input type="text" placeholder='请输入小组名称' confirm_type='search' value="{{inputVal}}" bindinput='inputBind' bindconfirm='query'></input>
    <image class="" src='../images/search.png' bindtap='query'></image>
  </view>


<!--打卡目标概要-->
<view class="windows">
  <view class="group_list_text" hidden='{{isBindExpert}}'>{{string}}</view>
  <navigator wx:for="{{groupList}}" hover-class='navigator-hover' url='../GroupDetail/GroupDetail'>
    <view class="group_list" style="color:{{item.color}};">
        <view class="lable1">
          <text class="groupS1">{{item.name}}</text>
        </view>
        <view class="lable2">
          <view class="groupS3">{{item.isClock}}</view>
        </view>
        <view class="lable2">
          <view class="groupS3">完成度：{{item.showCompletion}}%</view>
        </view>
       
      </view>
  </navigator>
</view>



<!--开始打卡按钮-->
<button type="default" bindtap="clock_start" plain="true" style=" width:50%; margin-top:30rpx; color:#77b5e7;border-color:#77b5e7;" hover-class="button-hover">开始打卡</button>